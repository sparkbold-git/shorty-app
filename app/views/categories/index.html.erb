<h1>Categories#index</h1>
<p>Find me in app/views/categories/index.html.erb</p>

<% @categories.each do |cat| %>
  <ul>
    <p><%= cat.name %></p>
    <% cat.urls.each do |url| %>
      <li><%= url.name %> - <%= link_to url.short_url, url.long_url %> - <%= url.long_url %></li>
      <% end %>
  </ul>
<% end %>
